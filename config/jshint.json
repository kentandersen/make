{
    "bitwise":      true,
    "camelcase":    true,
    "curly":        true,
    "eqeqeq":       true,
    "forin":        false,
    "immed":        true,
    "indent":       4,
    "latedef":      true,
    "newcap":       true,
    "noarg":        true,
    "noempty":      false,
    "nonew":        false,
    "plusplus":     false,
    "quotmark":     false,
    "undef":        true,
    "unused":       "vars",
    "strict":       false,
    "trailing":     true,
    "maxparams":    15,
    "maxdepth":     5,
    "maxstatements":    15,
    "maxcomplexity":    8,
    "maxlen":       120,
    "globals": {
        "window": true,
        "document": true,
        "define":   true,
        "requirejs": true
    }
}
